<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="KoboldPLAM"
   name="Kobold PLAM Plug-in"
   version="1.0.0"
   provider-name="Werkbold"
   class="kobold.client.plam.KoboldPLAMPlugin">

   <runtime>
      <library name="KoboldPLAM.jar"/>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.gef"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.swt"/>
   </requires>


   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Architecture Editor"
            icon="icons/sample.gif"
            extensions="kae"
            class="kobold.client.plam.editor.ArchitectureEditor"
            id="KoboldPLAM.editors.ArchitectureEditor">
      </editor>
   </extension>

</plugin>
