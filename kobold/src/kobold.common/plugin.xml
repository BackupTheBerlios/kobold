<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="kobold.commons"
   name="Kobold Commons Plug-in"
   version="1.0.0"
   provider-name="Werkbold">

   <runtime>
      <library name="kobold.common.jar">
         <export name="*"/>
         <packages prefixes="kobold.common"/>
      </library>
      <library name="contrib/gxl.jar">
         <export name="*"/>
      </library>
      <library name="contrib/dom4j/dom4j-1.5-beta-1.jar">
         <export name="*"/>
      </library>
      <library name="contrib/jakarta-commons/commons-logging.jar">
         <export name="*"/>
      </library>
      <library name="contrib/jakarta-commons/commons-logging-api.jar">
         <export name="*"/>
      </library>
      <library name="contrib/xmlrpc2-devel/xmlrpc-2.0-a1-dev.jar">
         <export name="*"/>
      </library>
      <library name="contrib/jakarta-commons/commons-codec-1.3-dev.jar">
         <export name="*"/>
      </library>
      <library name="contrib/jakarta-commons/commons-httpclient.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.osgi.services"/>
      <import plugin="org.eclipse.osgi.util"/>
      <import plugin="org.junit"/>
   </requires>


</plugin>
