    <authorPics type="map">
        <setting key="neco">neco.png</setting>
        <setting key="neccaino">neccaino.png</setting>
        <setting key="bettina">bettina.png</setting>
        <setting key="garbeam">garbeam.png</setting>
        <setting key="grosseml">grosseml.png</setting>
